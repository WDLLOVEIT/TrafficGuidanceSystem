<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pack.entity.Map" table="map">
        <id name="mapId" type="java.lang.Integer">
            <column name="mapId"/>
            <generator class="native"/>
        </id>
   
         
       <property name="coordinateId" type="java.lang.Integer">
            <column name="coordinateId" not-null="true"/>
        </property>
       <property name="zoom" type="java.lang.Integer">
            <column name="zoom" not-null="true"/>
        </property>
      
     </class>
</hibernate-mapping>