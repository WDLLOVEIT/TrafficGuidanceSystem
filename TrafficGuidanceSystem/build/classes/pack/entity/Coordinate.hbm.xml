<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pack.entity.Coordinate" table="coordinate">
        <id name="coordinateId" type="java.lang.Integer">
            <column name="coordinateId"/>
            <generator class="native"/>
        </id>
        <property name="abscissa" type="java.lang.Double">
            <column name="abscissa" not-null="true"/>
        </property>
        <property name="ordinate" type="java.lang.Double">
            <column name="ordinate" not-null="true"/>
        </property>
     </class>
</hibernate-mapping>